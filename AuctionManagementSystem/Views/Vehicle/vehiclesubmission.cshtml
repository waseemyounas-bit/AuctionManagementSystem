                $("#btn_VehicleOwnerShipHistoryY").css("background-color", "White");
                $("#btn_VehicleOwnerShipHistoryN").css("background-color", "rgb(225, 226, 227");
                $("#VehicleOwnerShipHistory").css("display", "none");
            });
            //End--->
            //Start--> Vehicle Accident
            $("#btn_VehicleAccidentY").click(function () {
                $("#btn_VehicleAccidentY").css("background-color", "rgb(225, 226, 227)");
                $("#btn_VehicleAccidentN").css("background-color", "White");
                $("#VehicleAccident").show();
            });
            $("#btn_VehicleAccidentN").click(function () {
                $("#btn_VehicleAccidentY").css("background-color", "White");
                $("#btn_VehicleAccidentN").css("background-color", "rgb(225, 226, 227");
                $("#VehicleAccident").css("display", "none");
            });
            //End--->
            
            //Start--> Vehicle dealership
            $("#btn_VehicleDmgRprY").click(function () {
                $("#btn_VehicleDmgRprY").css("background-color", "rgb(225, 226, 227)");
                $("#btn_VehicleDmgRprN").css("background-color", "White");
                $("#VehicleDamageRepair").show();
            });
            $("#btn_VehicleDmgRprN").click(function () {
                $("#btn_VehicleDmgRprY").css("background-color", "White");
                $("#btn_VehicleDmgRprN").css("background-color", "rgb(225, 226, 227");
                $("#VehicleDamageRepair").css("display", "none");
            });
            //End--->
            //Start--> Vehicle Damage Repair
            $("#rdrPrivate").click(function () {
                $("#PartyDealer").show();
                $("#PrivateDealer").hide();
            });
            $("#rdrseller").click(function () {
                $("#PrivateDealer").show();
                $("#PartyDealer").hide();
            });
            //End--->
            //Start--> Vehicle Owner
            $("#btn_VehicleEntitledonNameY").click(function () {
                $("#btn_VehicleEntitledonNameY").css("background-color", "White");
                $("#btn_VehicleEntitledonNameN").css("background-color", "rgb(225, 226, 227");
                $("#VehicleEntitledOwner").hide();
            });
            $("#btn_VehicleEntitledonNameN").click(function () {
                $("#VehicleEntitledOwner").show();
                $("#btn_VehicleEntitledonNameN").css("background-color", "White");
                $("#btn_VehicleEntitledonNameY").css("background-color", "rgb(225, 226, 227");
                $("#VehicleReserve").show();
            });
            //End--->
            //Start--> Reserve Non Reserve
            $("#NonReserve").click(function () {
                $("#VehicleReserve").hide();
            });
            $("#Reserve").click(function () {
                $("#VehicleReserve").show();
             });
            //End--->

            //Start--> Additional Info
            $("#AddInfoY").click(function () {
                debugger;
                $("#AddInfoR").show();
            });
            $("#AddInfoN").click(function () {
                $("#AddInfoR").hide();
            });
            //End--->
            var maxchars = 2;
            var maxYear = 4;
            $('#VehiclePurchaseMoth').keyup(function () {
                debugger;
                var tlength = $(this).val().length;
                $(this).val($(this).val().substring(0, maxchars));
                var tlength = $(this).val().length;
                remain = maxchars - parseInt(tlength);
                $('#VehiclePurchaseMoth').text(remain);
            });
            $('#VehiclePurchaseYear').keyup(function () {
                debugger;
                var tlength = $(this).val().length;
                $(this).val($(this).val().substring(0, maxYear));
                var tlength = $(this).val().length;
                remain = maxYear - parseInt(tlength);
                $('#VehiclePurchaseYear').text(remain);
            });
        });
    </script>
